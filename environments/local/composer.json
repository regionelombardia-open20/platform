{
  "name": "lispa/amos-basic-template",
  "description": "AMOS Basic Template",
  "keywords": [
    "amos4",
    "yii2",
    "framework",
    "advanced",
    "project template"
  ],
  "homepage": "http://www.lispa.it/",
  "type": "project",
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.4.0",
    "lispa/amos-admin": "~2.0",
    "lispa/amos-attachments": "~1.0.9",
    "lispa/amos-comuni": "~1.2.8",
    "lispa/amos-core": "~1.9.36",
    "lispa/amos-dashboard": "~1.8.3",
    "lispa/amos-email-manager": "~1.1.3",
    "lispa/amos-my-activities": "~1.0",
    "lispa/amos-notify": "~1.4",
    "lispa/amos-layout": "~1.0.0",
    "lispa/amos-upload": "~1.0.7",
    "lispa/amos-utility":"^1.0",
    "lispa/amos-wizflow": "~1.0",
    "lispa/amos-workflow": "~1.0",
    "lispa/amos-social-auth": "~1.0",
    "kartik-v/yii2-datecontrol": "@dev",
    "kartik-v/yii2-password": "@dev",
    "kartik-v/yii2-social": "@dev",
    "kartik-v/yii2-widgets": "@dev",
    "kartik-v/yii2-widget-select2": "2.0.8",
    "lajax/yii2-translate-manager": "1.*",
    "mdmsoft/yii2-admin": "~2.0",
    "schmunk42/yii2-giiant": "@stable",
    "yiisoft/yii2": "2.0.12",
    "yiisoft/yii2-bootstrap": "*",
    "yiisoft/yii2-swiftmailer": "*"
  },
  "require-dev": {
    "yiisoft/yii2-codeception": "*",
    "yiisoft/yii2-debug": "*",
    "yiisoft/yii2-gii": "*",
    "yiisoft/yii2-faker": "*"
  },
  "config": {
    "process-timeout": 1800,
    "secure-http": false
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
